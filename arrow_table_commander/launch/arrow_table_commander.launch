<launch>
  <!--path_planning_node-->
  <arg name="control_frequency" default="50" />
  <arg name="base_frame_id" default="base_footprint" />


    <!-- pot tf (temporary static) -->
  <node pkg="tf" type="static_transform_publisher" name="map_to_pot1" args="5.72 7.925 0 0 0 0 map pot1 1" />
  <node pkg="tf" type="static_transform_publisher" name="map_to_pot2" args="3.5 6.35 0 0 0 0 map pot2 1" />
  <node pkg="tf" type="static_transform_publisher" name="map_to_pot3" args="6.0 6.35 0 0 0 0 map pot3 1" />
  <node pkg="tf" type="static_transform_publisher" name="map_to_pot4" args="8.5 6.35 0 0 0 0 map pot4 1" />
  <node pkg="tf" type="static_transform_publisher" name="map_to_pot5" args="5.72 4.0 0 0 0 0 map pot5 1" />

  <node pkg="arrow_table_commander" type="arrow_table_commander" name="arrow_table_commander" output="screen">
    <param name= "control_frequency" value="$(arg control_frequency)" />
    <param name = "base_frame_id" value = "$(arg base_frame_id)" />
  </node>

</launch>
